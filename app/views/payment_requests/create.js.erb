wizard = new KTWizard('kt_wizard', {
	startStep: 1,
	clickableSteps: true,
});

<% if @payment_request.valid? %>
  $('form').attr('action', "<%= payment_request_path(@payment_request) %>").attr('method', 'put')
  wizard.goNext()
<% else %>
  toastr.error("<%=  @payment_request.errors.full_messages.to_sentence %>");
<% end %>