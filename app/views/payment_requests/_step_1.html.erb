
<div class="pb-5" data-wizard-type="step-content" data-wizard-state="current">
  <h3 class="mb-10 font-weight-bold text-dark">Enter Your Account Detail</h3>
  <!--begin::Input-->
  <div class="form-group">
    <label>First Name</label>
    <%= ff.text_field :first_name, class: 'form-control form-control-solid form-control-lg', value: current_user.first_name, disabled: true %>
    <span class="form-text text-muted">Please enter your first name.</span>
  </div>
  <!--end::Input-->
  <!--begin::Input-->
  <div class="form-group">
    <label>Last Name</label>
    <%= ff.text_field :last_name, class: 'form-control form-control-solid form-control-lg', value: current_user.last_name, disabled: true %>
    <span class="form-text text-muted">Please enter your last name.</span>
  </div>
  <!--end::Input-->
  <div class="row">
    <div class="col-xl-6">
      <!--begin::Input-->
      <div class="form-group">
        <label>Phone</label>
        <%= ff.text_field :phone_number, class: 'form-control form-control-solid form-control-lg', value: current_user.phone_number, disabled: true %>
        <span class="form-text text-muted">Please enter your phone number.</span>
      </div>
      <!--end::Input-->
    </div>
    <div class="col-xl-6">
      <!--begin::Input-->
      <div class="form-group">
        <label>Email</label>
        <%= ff.text_field :email, class: 'form-control form-control-solid form-control-lg', value: current_user.email, disabled: true %>
        <span class="form-text text-muted">Please enter your email address.</span>
      </div>
      <!--end::Input-->
    </div>
  </div>
</div>