<div class="nested-fields">
    <div class="form-group row">
      <div class="col-lg-1"></div>
      <div class="col-lg-2">
        <%= f.text_field :min_score, class: 'form-control form-control-lg form-control-solid required', placeholder:"Enter min score" %>
      </div>
      <div class="col-lg-2">
        <%= f.text_field :max_score, class: 'form-control form-control-lg form-control-solid required', placeholder:"Enter max score"%>
      </div>
      <div class="col-lg-2">
        <%= f.text_field :risk_value, class: 'form-control form-control-lg form-control-solid required', placeholder:"Enter risk value"%>
      </div>
      <div class="col-lg-2">
        <%= f.select :credit_tier, options_for_select(credit_tiers, f.object.credit_tier), { prompt: "Select Credit Tier" }, class: 'form-control form-control-lg form-control-solid required' %>
      </div>
      <div class="col-lg-2">
        <%= f.select :eleigible_for_offer, options_for_select([['Yes', true], ['No', false]], f.object.eleigible_for_offer), { prompt: "Select Eligible Offer" }, class: 'form-control form-control-lg form-control-solid required' %>
      </div>
        <%= link_to_remove_association f, class: "btn font-weight-bold  btn-icon" do %>
          <i class="la la-remove"></i>
        <% end %>
    </div>
</div>
